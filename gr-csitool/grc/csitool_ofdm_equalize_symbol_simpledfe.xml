<?xml version="1.0"?>
<block>
  <name>ofdm_equalize_symbol_simpledfe</name>
  <key>csitool_ofdm_equalize_symbol_simpledfe</key>
  <category>csitool</category>
  <import>import csitool</import>
  <make>csitool.ofdm_equalize_symbol_simpledfe($taps_tag)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>OFDM Taps Tag</name>
    <key>taps_tag</key>
    <type>string</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>64</vlen>
    <nports>1</nports>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>48</vlen>
    <nports>1</nports>
  </source>
</block>
